+ulong board_get_usable_ram_top(ulong total_size)
+{
+	/* Reserve 16M memory used by M core vring/buffer, which begins at 16MB before optee */
+	if (rom_pointer[1])
+		return gd->ram_top - SZ_16M;
+
+	return gd->ram_top;
+}
