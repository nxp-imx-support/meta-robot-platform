-    if (abs(theta) < Constants<Scalar>::epsilon()) {
-      Matrix3<Scalar> const V_inv = Matrix3<Scalar>::Identity() -
-                                    Scalar(0.5) * Omega +
-                                    Scalar(1. / 12.) * (Omega * Omega);
