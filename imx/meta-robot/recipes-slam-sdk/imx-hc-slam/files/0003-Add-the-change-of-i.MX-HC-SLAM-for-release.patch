+#include <gst/gst.h>
+
+using namespace std;
+
+cv::Mat new_frame;
+std::mutex new_frame_mtx;
+int new_frame_avail = 0;
+GMainLoop* loop;
+
+static GstFlowReturn
+new_sample(GstElement* sink, gpointer* data)
+{
+    GstSample* sample;
+    //cout << "get new sample" << endl;
+    g_signal_emit_by_name(sink, "pull-sample", &sample);
+    if (!sample) return GST_FLOW_ERROR;
+
+    GstCaps*      caps       = gst_sample_get_caps(sample);
+    GstStructure* gst_struct = gst_caps_get_structure(caps, 0);
+
+    int width, height;
+    gst_structure_get_int(gst_struct, "width", &width);
+    gst_structure_get_int(gst_struct, "height", &height);
+
+
+    GstBuffer* buffer = gst_sample_get_buffer(sample);
+    if (!buffer) {
+        g_printerr("Unable to get buffer from sample\n");
+        gst_sample_unref(sample);
+        return GST_FLOW_ERROR;
+    }
+
+    GstMapInfo mapinfo;
+    if (!gst_buffer_map(buffer, &mapinfo, GST_MAP_READ)) {
+        g_printerr("Unable to map video frame\n");
+        gst_sample_unref(sample);
+        return GST_FLOW_ERROR;
+    }
+    
+    cv::Mat frame = cv::Mat(cv::Size(width, height), CV_8UC2, (char *)mapinfo.data);
+
+    if(new_frame_mtx.try_lock()){
+        //cv::cvtColor(frame, new_frame, cv::COLOR_YUV2GRAY_YUY2);
+        cv::cvtColor(frame, new_frame, cv::COLOR_YUV2RGB_YUY2);
+        new_frame_avail = 1;
+        //cv::imwrite("output.png", new_frame);
+        //cout << "saved the debug image from appsink!" << endl;
+        new_frame_mtx.unlock();
+    }
+
+    gst_buffer_unmap(buffer, &mapinfo);
+    gst_sample_unref(sample);
+
+    return GST_FLOW_OK;
+}
+
+/*
+void run_Gstloop()
+{
+    cout << "Running gstreamer loop..." << endl;
+
+    g_main_loop_run(loop);
+
+    cout << "Stop gstreamer loop" << endl;
+
+}
+*/
+
+int main(int argc, char **argv)
+{  
+    if(argc < 4)
+    {
+        cerr << endl << "Usage: ./imx_mono_camera path_to_vocabulary path_to_settings path_to_camera_dev (trajectory_file_name)" << endl;
+        return 1;
+    }
+
+    string vocab_path;
+    string cam_setting_path;
+    string file_name, camera_dev_name;
+    int video_width = 640;
+    int video_height = 480;
+
+    vocab_path = string(argv[1]);
+    cout << "Vocabulary path is " << vocab_path << endl;
+    cam_setting_path = string(argv[2]);
+    cout << "camera setting path is " << cam_setting_path << endl;
+    camera_dev_name = string(argv[3]);
+    cout << "Using camera device: " << camera_dev_name << endl;
+    file_name = string(argv[4]);
+    cout << "file name: " << file_name << endl;
+
+    //init gstreamer
+    gst_init(&argc, &argv);
+    loop = g_main_loop_new(NULL, FALSE);
+    if (!loop) {
+        g_printerr("failed to create main loop\n");
+        return EXIT_FAILURE;
+    }
+
+    GstElement* source = gst_element_factory_make("v4l2src", "source");
+    GstElement* filter = gst_element_factory_make("capsfilter", "filter");
+    GstElement* appsink = gst_element_factory_make("appsink", "appsink");
+    GstElement* cam_pipe = gst_pipeline_new ("camera-pipeline");
+
+    if (!source || !filter || !appsink || !cam_pipe) {
+        g_printerr("Failed to create elements for pipeline\n");
+        return EXIT_FAILURE;
+    }
+
+    //set source device
+    g_object_set(G_OBJECT(source), "device", argv[argc-2], NULL);
+
+    //set filter
+    const char* format = "YUY2";
+    GstCaps* filter_caps = gst_caps_new_simple("video/x-raw",
+                                "format",G_TYPE_STRING, format,
+                                "width", G_TYPE_INT, video_width,
+                                "height", G_TYPE_INT, video_height,
+                                NULL);
+    g_object_set(G_OBJECT(filter), 
+		 "caps", filter_caps, 
+		 NULL);
+    gst_caps_unref(filter_caps);
+
+    GstCaps* appsink_caps = gst_caps_new_simple("video/x-raw",
+                                "format", G_TYPE_STRING, format,
+                                 "width", G_TYPE_INT, video_width,
+                                "height", G_TYPE_INT, video_height,
+                                NULL);
+    //filter has no property name drop,  but appsink have this.
+    g_object_set(G_OBJECT(appsink),
+                 "caps", appsink_caps,
+                 "drop", FALSE,
+                 "emit-signals", TRUE,
+                 NULL);
+    gst_caps_unref(appsink_caps);
+
+    /* create callback funtion */
+    g_signal_connect(appsink, "new-sample", G_CALLBACK(new_sample), NULL);
+
+    gst_bin_add_many(GST_BIN(cam_pipe),
+                         source,
+                         filter,
+                         appsink,
+                         NULL);
+
+    if (!gst_element_link_many(source, filter, appsink, NULL)) {
+            g_printerr("Failed to link input ssdcam\n");
+            return EXIT_FAILURE;
+    }
+
+    //create SLAM
+    imx_hc_slam::System myslam(vocab_path, cam_setting_path, imx_hc_slam::System::MONOCULAR, true, 0, file_name);
+    
+
+    /* Start playing the pipeline */
+    gst_element_set_state (cam_pipe, GST_STATE_PLAYING);
+
+    //seems that we don't need to start a GstLoop, and it will have conflict with cv::imshow
+    //std::thread Gstloop_thread (run_Gstloop);
+    //cout << "run_Gstloop thread started..." << endl;
+
+    double tframe;
+    cout.precision(17);
+
+    while (1)
+    {
+        tframe = std::chrono::duration_cast<std::chrono::microseconds>(std::chrono::system_clock::now().time_since_epoch()).count();
+        tframe = 1e-6 * tframe;
+        //cout << "tframe: " << tframe << endl;
+        new_frame_mtx.lock();
+        if (new_frame_avail == 1)
+        {
+            //cv::Mat frm = cv::imread("output.png", cv::IMREAD_UNCHANGED);
+            myslam.TrackMonocular(new_frame, tframe, imx_hc_slam::Settings::COMPUTE_TYPE_NEON_OPENCL);
+            new_frame_avail = 0;
+            new_frame_mtx.unlock();
+            //usleep(5000);
+        }
+        else
+        {
+            new_frame_mtx.unlock();
+	    //sleep for a short time in case the new_sample function can never get the lock
+            usleep(5000);
+        }
+    }
+    //FIXME
+    //shutdown SLAM and save the map
+
+    gst_element_set_state(cam_pipe, GST_STATE_NULL);
+    gst_object_unref(GST_OBJECT(cam_pipe));
+    g_main_loop_unref(loop);
+
+    return 0;
+}
