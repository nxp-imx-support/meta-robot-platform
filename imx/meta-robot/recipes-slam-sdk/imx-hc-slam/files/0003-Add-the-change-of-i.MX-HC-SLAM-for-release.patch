-  ///                | -b  a  d |
-  ///
-  SOPHUS_FUNC static Tangent vee(Transformation const& Omega) {
-    using std::abs;
-    return Tangent(Omega(2, 1), Omega(0, 2), Omega(1, 0), Omega(0, 0));
-  }
