-void RobustKernelTukey::robustify(double e, Eigen::Vector3d& rho) const
-{
-  if (e <= _deltaSqr) { // inlier
-    double factor = e*_invDeltaSqr;
-    double d = 1-factor;
-    double dd = d*d;
-    rho[0] = _deltaSqr*(1-dd*d);
-    rho[1] = 3*dd;
-    rho[2] = -6*_invDeltaSqr*d;
-  } else { // outlier
-    rho[0] = _deltaSqr; // rho(e)   = delta^2
-    rho[1] = 0.;
-    rho[2] = 0.;   
-  }
