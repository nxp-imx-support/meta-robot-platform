-  double aux1 = dsqrReci * e2 + 1.0;
-  double aux2 = sqrt(aux1);
-  rho[0] = 2 * dsqr * (aux2 - 1);
-  rho[1] = 1. / aux2;
-  rho[2] = -0.5 * dsqrReci * rho[1] / aux1;
-}
