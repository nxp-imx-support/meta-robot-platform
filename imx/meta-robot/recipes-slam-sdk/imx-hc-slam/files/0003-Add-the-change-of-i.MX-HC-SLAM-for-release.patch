-  SOPHUS_ENSURE(yDirHint_foo_sqr_length > Constants<T>::epsilon(), "%",
-                yDirHint_foo.transpose());
-  SOPHUS_ENSURE(normal_foo_sqr_length > Constants<T>::epsilon(), "%",
