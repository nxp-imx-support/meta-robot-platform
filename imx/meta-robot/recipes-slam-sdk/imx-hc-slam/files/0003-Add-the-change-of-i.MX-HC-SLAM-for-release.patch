-  /// As above, but also returns ``theta = |omega|``.
-  ///
-  SOPHUS_FUNC TangentAndTheta logAndTheta() const {
-    TangentAndTheta J;
-    using std::abs;
-    using std::atan;
-    using std::sqrt;
-    Scalar squared_n = unit_quaternion().vec().squaredNorm();
-    Scalar w = unit_quaternion().w();
-
-    Scalar two_atan_nbyw_by_n;
