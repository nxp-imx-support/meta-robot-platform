-            (foo_T_newaverage.inverse() * foo_T_average).log()) <
-        Constants<Scalar>::epsilon()) {
-      return foo_T_newaverage;
-    }
-
-    foo_T_average = foo_T_newaverage;
-  }
-  // LCOV_EXCL_START
-  return nullopt;
-  // LCOV_EXCL_STOP
