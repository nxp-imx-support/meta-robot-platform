+    int ORBextractor::operator()( InputArray _image, InputArray _mask, vector<KeyPoint>& _keypoints,
+                                  OutputArray _descriptors, std::vector<int> &vLappingArea, const int ComputeType)
+    {
+        //cout << "[ORBextractor]: Max Features: " << nfeatures << endl;
+        if(_image.empty())
+            return -1;
+
+        Mat image = _image.getMat();
+        if (image.type() != CV_8UC1)
+        {
+            std::cerr << std::endl << "Image type : " << image.type() << std::endl;
+            return -1;
+        }
+#ifdef ENABLE_SEG
+        // Start to do image semantic segmentation
+        std::thread seg_thread = std::thread(&ORBextractor::RunSemanticSegmentation, this);
+#endif
+        // Pre-compute the scale pyramid
+        ComputePyramid(image);
+
+        //vector < vector<KeyPoint> > allKeypoints;
+        allKeypoints.clear();
+        allKeypoints.resize(nlevels);
+
+#ifdef TIME_DEBUG
+        std::chrono::steady_clock::time_point time_StartCompKeyPoints = std::chrono::steady_clock::now();
+#endif
+        ComputeKeyPointsOctTree(allKeypoints, ComputeType);
+
+#ifdef TIME_DEBUG
+        std::chrono::steady_clock::time_point time_EndCompKeyPoints = std::chrono::steady_clock::now();
+        double TimeComp_KeyPoints = std::chrono::duration_cast<std::chrono::duration<double,std::milli> >(time_EndCompKeyPoints - time_StartCompKeyPoints).count();
+        cout << "[ORBextractor]: ComputeKeyPoints time: " << TimeComp_KeyPoints << endl;
+        std::chrono::steady_clock::time_point time_StartCompDescriptors = std::chrono::steady_clock::now();
+#endif
+
+        Mat descriptors;
+        allDescriptors.clear();
+        allDescriptors.reserve(nlevels);
+
+        int nkeypoints = 0;
+        for (int level = 0; level < nlevels; ++level)
+        {
+            int nkeypointsLevel = allKeypoints[level].size();
+            nkeypoints += nkeypointsLevel;
+    	    if (nkeypointsLevel == 0)
+            {
+                Mat desc = cv::Mat(1, 32, CV_8UC1); //in case nkeypointsLevel is 0 and cause error
+                desc = Mat::zeros(1, 32, CV_8UC1);
+                allDescriptors.push_back(desc);
+            }
+    	    else
+            {
+                Mat desc = cv::Mat(nkeypointsLevel, 32, CV_8UC1);
+                desc = Mat::zeros(nkeypointsLevel, 32, CV_8UC1);
+                allDescriptors.push_back(desc);
+            }
+        }
+        if(0 == nkeypoints)
+            _descriptors.release();
+        else
+        {
+            _descriptors.create(nkeypoints, 32, CV_8U); //create rows x cols array, nkeypoints x 32
+            descriptors = _descriptors.getMat();
+        }
+
+        _keypoints = vector<cv::KeyPoint>(nkeypoints);
+    
+        for(int i = 0; i < nlevels; i++)
+            ComputeDescriptors_level(i, ComputeType);
+
+#ifdef ENABLE_SEG
+        // Wait for image semantic segmentation to finish
+        seg_thread.join();
+#endif
+        //int offset = 0;
+        //Modified for speeding up stereo fisheye matching
+        int monoIndex = 0, stereoIndex = nkeypoints-1;
+        for (int level = 0; level < nlevels; ++level)
+        {
+            vector<KeyPoint>& keypoints = allKeypoints[level];
+            int nkeypointsLevel = (int)keypoints.size();
+
+            if(0 == nkeypointsLevel)
+                continue;
+
+
+            float scale = mvScaleFactor[level]; //getScale(level, firstLevel, scaleFactor);
+            int i = 0;
+            for (vector<KeyPoint>::iterator keypoint = keypoints.begin(),
+                         keypointEnd = keypoints.end(); keypoint != keypointEnd; ++keypoint){
+
+                // Scale keypoint coordinates
+                if (level != 0){
+                    keypoint->pt *= scale;
+                }
+
+#ifdef ENABLE_SEG
+                //Add semantic segmentation filter
+                //In 21 classes of objects, person is class 15
+                if (classes_found[15] != 0)
+                {
+                    // If the keypoint pixel is recognized as "person", discard this keypoint
+                    int seg_x, seg_y;
+                    seg_x = cvRound((float)keypoint->pt.x * 256 / (float)image.cols);
+                    seg_y = cvRound((float)keypoint->pt.y * 256 / (float)image.rows);
+                    if (mSegmented_image.at<uchar>(seg_y, seg_x) == 15)
+                    {
+                        i++;
+                        continue;
+                    }
+                }
+#endif
+                if(keypoint->pt.x >= vLappingArea[0] && keypoint->pt.x <= vLappingArea[1]){
+                    _keypoints.at(stereoIndex) = (*keypoint);
+                    allDescriptors[level].row(i).copyTo(descriptors.row(stereoIndex));
+                    stereoIndex--;
+                }
+                else{
+                    _keypoints.at(monoIndex) = (*keypoint);
+                    allDescriptors[level].row(i).copyTo(descriptors.row(monoIndex));
+                    monoIndex++;
+                }
+                i++;
+            }
+        }
+
+#ifdef TIME_DEBUG
+        std::chrono::steady_clock::time_point time_EndCompDescriptors = std::chrono::steady_clock::now();
+        double TimeComp_Descriptors = std::chrono::duration_cast<std::chrono::duration<double,std::milli> >(time_EndCompDescriptors - time_StartCompDescriptors).count();
+        cout << "[ORBextractor]: ComputeDescriptors time: " << TimeComp_Descriptors << endl;
+#endif
+        //cout << "[ORBextractor]: extracted " << _keypoints.size() << " KeyPoints" << endl;
+        return monoIndex;
+    }
