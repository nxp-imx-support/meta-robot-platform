-///
-template <class T>
-Matrix3<T> rotationFromNormal(Vector3<T> const& normal_foo,
-                              Vector3<T> xDirHint_foo = Vector3<T>(T(1), T(0),
-                                                                   T(0)),
-                              Vector3<T> yDirHint_foo = Vector3<T>(T(0), T(1),
-                                                                   T(0))) {
