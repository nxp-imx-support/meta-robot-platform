-      upsilon_omega.template head<3>() = V_inv * translation();
-    } else {
-      Scalar const half_theta = Scalar(0.5) * theta;
-
-      Matrix3<Scalar> const V_inv =
-          (Matrix3<Scalar>::Identity() - Scalar(0.5) * Omega +
-           (Scalar(1) -
