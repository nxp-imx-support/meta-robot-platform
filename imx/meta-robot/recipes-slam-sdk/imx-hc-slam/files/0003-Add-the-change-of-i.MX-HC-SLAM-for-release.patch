-  m_weighting = (WeightingType)((int)fvoc["weightingType"]);
-  
-  createScoringObject();
-
-  // nodes
-  cv::FileNode fn = fvoc["nodes"];
-
-  m_nodes.resize(fn.size() + 1); // +1 to include root
-  m_nodes[0].id = 0;
-
-  for(unsigned int i = 0; i < fn.size(); ++i)
